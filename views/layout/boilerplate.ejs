        <script>// open chat
        function openChat(){
          
          $(".chatBox").show(500);
          $(".chatBtn").addClass('hide');
          $(".chatBtn").removeClass('show');
        }


        function closeChat(){
          
          $(".chatBox").hide(500);
          $(".chatBtn").addClass('show');
          $(".chatBtn").removeClass('hide');
        }

        function sendNotification() {
        Push.create("Hello, World!", {
          body: "This is a push notification",
          icon: "/path/to/icon.png",
          timeout: 4000,
          onClick: function () {
            window.focus();
            this.close();
          }
        });
      }</script>
   
